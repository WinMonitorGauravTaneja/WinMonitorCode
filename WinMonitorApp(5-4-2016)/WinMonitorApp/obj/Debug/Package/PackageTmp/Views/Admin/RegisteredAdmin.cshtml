@using WinMonitorApp.Models;

<h5 class="PageHeadings">Registered Administrator List</h5>
<br><br><br><br>

<div class="table-responsive">
<table class="table table-hover" id="RegisteredAdminTable">
    <thead>
        <tr>
            <td>
                Admin Name
            </td>
            <td>
                Account Type
            </td>
            <td>
                Current Status
            </td>
        </tr>
    </thead>
    <tbody>
      @foreach (var Admin in Model)
      {
          <tr style="font-size:medium; font-weight:500; color:black;">
              <td >@Admin.DBUsername</td>
              <td>@Admin.DBAccountType</td>
              <td>Active/Inactive</td>
          </tr>
      }
    </tbody>
</table>
</div>

<!--to add custom.js functions after body load-->
<script src="~/Scripts/custom.js"></script>
