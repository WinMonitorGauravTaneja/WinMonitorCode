@using WinMonitorApp.Models;

<div class="PageHeadings">Registered Administrator List</div>
<br><br><br><br>

<div class="table-responsive">
<table class="table table-striped table-hover" id="RegisteredAdminTable">
    <thead>
        <tr>
            <td>
                Admin Name
            </td>
            <td>
                Account Type
            </td>
            <td>
                Current Status
            </td>
        </tr>
    </thead>
    <tbody>
      @foreach (var Admin in Model)
      {
          <tr style="background-color:rgb(255,255,250);">
              <td >@Admin.DBUsername</td>
              <td>@Admin.DBAccountType</td>
              <td>Active/Inactive</td>
          </tr>
      }
    </tbody>
</table>
</div>

<!--to add custom.js functions after body load-->
<script src="~/Scripts/custom.js"></script>
